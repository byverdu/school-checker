{
  "compilerOptions": {
      "baseUrl": "./app",
      "paths": {
        "shared-data/*": ["../shared-data/*"],
        "Components/*": ["client/Components/*"],
        "UtilsUI/*": ["client/Utils/*"],
        "static/*": ["../static/*"],
      },
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "lib": [
        "dom",
        "es2015",
      ],
      "types":[
        "node",
        "googlemaps"
      ]
  }
}